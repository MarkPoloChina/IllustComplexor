{
  "name": "icxor",
  "version": "0.2.9",
  "private": true,
  "author": {
    "name": "MarkPolo"
  },
  "description": "ICXOR (Illust Complexor) is a tool built by electron to manage illusts at both local and remote.",
  "license": "GPL v3.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "vue-cli-service electron:serve",
    "build-icon": "electron-icon-builder --input=./public/icon.png --output=build/common --flatten",
    "build-icon-mac": "electron-icon-builder --input=./public/icon_mac.png --output=build/mac --flatten"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@vueuse/core": "^9.4.0",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "element-plus": "^2.2.8",
    "fs-extra": "^10.1.0",
    "lowdb": "1.0.0",
    "node-sass": "^7.0.1",
    "sass-resources-loader": "^2.2.5",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.32.7",
    "sass-loader": "13.0.2",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "packageManager": "yarn@3.2.1"
}